{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "eqeqeq": ["error", "smart"],
    "no-console": "off",
    "no-unused-vars": "warn"
  }
}
